---
import Header from '../components/Header.astro';
import Banner from '../components/Banner.jsx';
import Layout from '../layouts/Layout.astro';
import ServiceSection from '../components/ServiceSection.jsx';

import TeamBenefits from '../components/TeamBenefits.tsx';
import CourseOffer from '../components/CourseOffer.astro';

import Footer from '../components/Footer.astro';

import TeamMembers from '../components/TeamMembers.tsx';  
import { teamMembers } from '../components/teamMembersData';
import '../styles/global.css';
---

<Layout>
  <Header />
  <Banner />
  <ServiceSection client:load />
     <TeamMembers members={teamMembers} client:load />
     <TeamBenefits client:load />
     <CourseOffer />
     <Footer />
</Layout>
